{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/preset-stage-0"
  ],
  "plugins": [
    [
      "emotion", {
        "inline": true
      }
    ],
    [
      "@babel/plugin-transform-runtime", {
        "helpers": false,
        "polyfill": false
      },
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
